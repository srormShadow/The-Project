// this program can run even if we get the numbers from the user
//this can also be done using * and /
#include<stdio.h>      
 int main()    
{    
int a=10, b=20;      
printf("Before swapping a=%d b=%d",a,b);      

a=a+b;    //a=30    
b=a-b;    //b=10    got the value of b
a=a-b;    //a=20    got the value of a

printf("\nAfter swap a=%d b=%d",a,b);  
return 0;  
}   
